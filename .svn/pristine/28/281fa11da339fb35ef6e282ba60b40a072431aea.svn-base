package org.apache.giraph.plan;

public class RawQueryNode {
	private int vid;
	private int vlabel;
	private int nextVid;
	private int elabel;
	private EdgeDirection edgeDir;
	
	public RawQueryNode(int vid, int vlabel, int nextVid, int elabel, EdgeDirection edgeDirection) {
		this.vid = vid;
		this.vlabel = vlabel;
		this.nextVid = nextVid;
		this.elabel = elabel;
		this.edgeDir = edgeDirection;
	}
	
	public int getVid() {
		return vid;
	}
	
	public int getVlabel() {
		return vlabel;
	}
	
	public int getNextVid() {
		return nextVid;
	}
	
	public int getElabel() {
		return elabel;
	}
	
	public EdgeDirection getEdgeDirection() {
		return edgeDir;
	}
}
