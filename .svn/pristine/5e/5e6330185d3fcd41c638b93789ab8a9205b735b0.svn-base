package org.apache.giraph.subgraph.graphextraction;

import java.io.DataInput;
import java.io.DataOutput;
import java.io.IOException;

import org.apache.hadoop.io.WritableComparable;

/**
 * In this prototype, we assume the aggregated value (weight) is Integer.
 * NOTE THAT this class is also used to represent the message during preprocess, the mapping is
 * 	-- sid    ==> vid, the id of vertex
 *  -- tid    ==> vlabel, the id of vertex label
 *  -- qid    ==> elabel, the id of outgoing edge label
 *  -- weight ==> weight, the initial weight of the outgoing edge label 
 * @author yxshao
 *
 */
public class artialAggregatedPathBatch implements WritableComparable<artialAggregatedPathBatch> {

	int sid; /* vid */
	int tid; /* vlabel */
	int qid; /* elabel */
	int weight;
	
	public artialAggregatedPathBatch() {
//		weight = new IntWritable();
	}
	
	public int getVid() {
		return sid;
	}
	
	public void setVid(int vid) {
		this.sid = vid;
	}
	
	public int getSid() {
		return sid;
	}
	
	public void setSid(int sid) {
		this.sid = sid;
	}
	
	public int getVlabel() {
		return tid;
	}
	
	public void setVlabel(int vlabel) {
		tid = vlabel;
	}
	
	public int getTid() {
		return tid;
	}
	
	public void setTid(int tid) {
		this.tid = tid;
	}
	
	public int getElabel() {
		return qid;
	}
	
	public void setElabel(int elabel) {
		this.qid = elabel;
	}
	
	public int getQid() {
		return qid;
	}
	
	public void setQid(int qid) {
		this.qid = qid;
	}
	
	public int getWeight() {
		return weight;
	}
	
	public void setWeight(int weight) {
		this.weight = weight;
	}
	
	@Override
	public void readFields(DataInput in) throws IOException {
		sid = in.readInt();
		tid = in.readInt();
		qid = in.readInt();
		weight = in.readInt();
//		weight = new IntWritable();
//		weight.readFields(in);
	}

	@Override
	public void write(DataOutput out) throws IOException {
		out.writeInt(sid);
		out.writeInt(tid);
		out.writeInt(qid);
		out.writeInt(weight);
//		weight.write(out);
	}

	@Override
	public int compareTo(artialAggregatedPathBatch o) {
		return 0;
	}
}
