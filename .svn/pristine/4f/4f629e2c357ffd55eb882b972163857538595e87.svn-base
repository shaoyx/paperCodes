package org.apache.giraph.subgraph.graphextraction;


public class BasicEdge {
		
	private int vid;
	
	private int label;
	private int weight;
	
	public BasicEdge() { }
	
	public BasicEdge(int vid, int label, int weight){
		this.initialize(vid, label, weight);
	}
	
	public void initialize(int vid, int label, int weight) {
		this.vid = vid;
		this.label = label;
		this.weight = weight;
	}
	
	public int getLabel() {
		return this.label;
	}
	
	public void setLabel(int label) {
		this.label = label;
	}
	
	public int getWeight() {
		return weight;
	}
	
	public void setWeight(int weight) {
		this.weight = weight;
	}
	
	public int getVid() {
		return vid;
	}
	
	public void setVid(int vid) {
		this.vid = vid;
	}
	
	public String toString() {
		return "(vid="+vid+" label="+label+" weight="+weight+")";
	}
}
