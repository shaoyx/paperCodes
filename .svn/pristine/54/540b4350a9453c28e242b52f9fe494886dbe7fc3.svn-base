package org.apache.giraph.subgraph.graphextraction;

public class Pair {
	private int first;
	private int second;
	
	public Pair() {}
	
	public Pair(int first, int second) {
		this.first = first;
		this.second = second;
	}
	
	public void setFirst(int first) {
		this.first = first;
	}
	
	public int getFirst() {
		return first;
	}
	
	public void setSecond(int second) {
		this.second = second;
	}
	
	public int getSecond() {
		return this.second;
	}
}
